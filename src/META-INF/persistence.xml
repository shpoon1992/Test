<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="persistenceUnit">
        <class>model.Answer</class>
        <class>model.Question</class>
        <class>model.Level</class>
        <class>model.Member</class>
        <class>model.Topic</class>
        <class>model.Account</class>
        <class>model.Team</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/smuctf"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="eclipselink.jdbc.user" value="root"/>
            <property name="eclipselink.jdbc.password" value="mysql"/>
        </properties>
    </persistence-unit>
</persistence>